msgstr ""

#: ../../source/ambd_arduino/AMB21/api_documents/Class BLEAdvert.rst:158
msgid "Set the BLE advertising type."
msgstr ""

#: ../../source/ambd_arduino/AMB21/api_documents/Class BLEAdvert.rst:168
msgid "advType: the desired advertisement type. Valid values:"
msgstr ""

#: ../../source/ambd_arduino/AMB21/api_documents/Class BLEAdvert.rst:171
msgid "– 0 = GAP_ADTYPE_ADV_IND : connectable undirected advertisement"
msgstr ""

#: ../../source/ambd_arduino/AMB21/api_documents/Class BLEAdvert.rst:173
msgid "– 1 = GAP_ADTYPE_ADV_HDC_DIRECT_IND : connectable high duty cycle directed"
msgstr ""

#: ../../source/ambd_arduino/AMB21/api_documents/Class BLEAdvert.rst:176
msgid "– 2 = GAP_ADTYPE_ADV_SCAN_IND : scannable undirected advertisement"
msgstr ""

#: ../../source/ambd_arduino/AMB21/api_documents/Class BLEAdvert.rst:178
msgid ""
"– 3 = GAP_ADTYPE_ADV_NONCONN_IND : Non-connectable undirected "
"advertisement"
msgstr ""

#: ../../source/ambd_arduino/AMB21/api_documents/Class BLEAdvert.rst:181
msgid ""
"– 4 = GAP_ADTYPE_ADV_LDC_DIRECT_IND : connectable low duty cycle directed"
" advertisement"
msgstr ""

